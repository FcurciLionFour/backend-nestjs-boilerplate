NODE_ENV=development
PORT=3000

FRONTEND_URL=http://localhost:4200
CORS_ORIGINS=http://localhost:4200
COOKIE_SAME_SITE=lax
COOKIE_SECURE=false
COOKIE_CSRF_MAX_AGE_MS=7200000
COOKIE_REFRESH_MAX_AGE_MS=604800000
TRUST_PROXY=false
HTTP_BODY_LIMIT=1mb
HTTP_URLENCODED_LIMIT=1mb
SWAGGER_ENABLED=true
SWAGGER_PATH=docs

DATABASE_URL=postgresql://user:password@localhost:5432/app
SEED_ADMIN_EMAIL=

JWT_ACCESS_SECRET=change-me-access-secret-at-least-32-characters
JWT_REFRESH_SECRET=change-me-refresh-secret-at-least-32-characters
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d
